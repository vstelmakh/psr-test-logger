{
    "name": "vstelmakh/test-logger",
    "description": "Library providing PSR-3 compliant test logger",
    "keywords": ["php", "log", "logger", "psr-3", "logging", "test"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/vstelmakh/test-logger",
    "authors": [
        {
            "name": "Volodymyr Stelmakh",
            "homepage": "https://github.com/vstelmakh"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "VStelmakh\\TestLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VStelmakh\\TestLogger\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
